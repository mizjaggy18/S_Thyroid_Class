{
  "name": "S_Thyroid_Class",
  "description": "Classification of MGG stain to Normal and Abnormal (Thyroid Cancer)",
  "container-image": {
    "image": "mizjaggy18/s_thyroid_class",
    "type": "singularity"
  },
  "command-line": "echo [CYTOMINE_HOST] [CYTOMINE_PUBLIC_KEY] [CYTOMINE_PRIVATE_KEY] [CYTOMINE_ID_PROJECT] [CYTOMINE_ID_SOFTWARE] [CYTOMINE_ID_IMAGES] [CYTOMINE_ID_ROI_TERM] [CYTOMINE_ID_C0_TERM] [CYTOMINE_ID_C1_TERM] [MODEL_SELECT] [BATCH_SIZE] [LOG_LEVEL]",
  "inputs": [
    {
      "id": "cytomine_host",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine host",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_public_key",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine public key",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_private_key",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine private key",
      "set-by-server": true,
      "optional": false,
      "type": "String"
    },
    {
      "id": "cytomine_id_project",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine project ID",
      "set-by-server": true,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "cytomine_id_software",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine software ID",
      "set-by-server": true,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "cytomine_id_images",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine Image IDs",
      "description": "Images on which to classify objects",
      "optional": false,
      "type": "ListDomain",
      "uri": "/api/project/$currentProject$/imageinstance.json",
      "uri-print-attribute": "instanceFilename",
      "uri-sort-attribute": "created"
    },
	{
	    "id": "cytomine_id_roi_term",
	    "value-key": "[@ID]",
	    "command-line-flag": "--@id",
	    "name": "Cytomine ROI/patch term ID",
	    "description": "Ontology term ID for ROI/patch to be classified",
	    "set-by-server": false,
	    "optional": false,
	    "type": "Domain",
	    "uri": "/api/ontology/$currentOntology$/term.json",
	    "uri-print-attribute": "name",
	    "uri-sort-attribute": "name"
	},
    {
      "id": "cytomine_id_c0_term",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine class0 (Tumor) cell term ID",
      "description": "Ontology term ID for the cells that will be classified as Class 0 (Tumor)",
      "optional": false,
      "type": "Domain",
      "uri": "/api/ontology/$currentOntology$/term.json",
      "uri-print-attribute": "name",
      "uri-sort-attribute": "name"
    },
    {
      "id": "cytomine_id_c1_term",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Cytomine class1 (Normal) cell term ID",
      "description": "Ontology term ID for the cells that will be classified as Class 1 (Normal)",
      "optional": false,
      "type": "Domain",
      "uri": "/api/ontology/$currentOntology$/term.json",
      "uri-print-attribute": "name",
      "uri-sort-attribute": "name"
    },
    {
      "id": "model_select",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Model Select",
      "description": "1: PyTorch, 2: OpenVino",
      "default-value": 1,
      "set-by-server": false,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "batch_size",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "name": "Batch size",
      "description": "Batch size for batch processing",
      "default-value": 32,
      "set-by-server": false,
      "optional": false,
      "type": "Number"
    },
    {
      "id": "log_level",
      "value-key": "[@ID]",
      "command-line-flag": "--@id",
      "default": "WARNING",
      "name": "Logging verbosity",
      "description": "The level is given by a value among 'DEBUG', 'INFO', 'WARNING', 'ERROR', 'CRITICAL'",
      "set-by-server": false,
      "optional": true,
      "type": "String",
      "default-value": "WARNING"
    }
  ],

  "schema-version": "cytomine-0.1"
}
